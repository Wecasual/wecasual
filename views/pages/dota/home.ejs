    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-body">
          <h5><%= message%></h5>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" id="modal-close">Close</button>
        </div>
